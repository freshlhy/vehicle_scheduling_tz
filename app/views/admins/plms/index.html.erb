<% provide(:title, '车辆周期管理') %>

<div class="content span9">
  <h4>车辆周期管理</h4>

  <div class="row">
    <div class="span9">
      <% flash.each do |key, value| %>
          <%= content_tag(:div, value, class: "alert alert-#{key}") %>
      <% end %>
    </div>
  </div>
  <div class="row">
    <div class="span9">
      <table id="plms" class="table table-hover table-striped table-bordered" cellpadding="0" cellspacing="0"
             border="0" data-source="<%= admins_plms_url(format: "json") %>">
        <thead>
        <tr class="alert alert-success">
          <th>维护车辆</th>
          <th>周期类型</th>
          <th>维护周期</th>
          <th>开始时间</th>
          <th>上次维护</th>
          <th>下次维护</th>
          <th>是否激活</th>
          <th>更改</th>
        </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
  </div>
</div>

