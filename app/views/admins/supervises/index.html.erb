<% provide(:title, '部件监督管理') %>

<div class="content span9">
  <h4>部件监督管理</h4>

  <div class="row">
    <div class="span9">
      <% flash.each do |key, value| %>
          <%= content_tag(:div, value, class: "alert alert-#{key}") %>
      <% end %>
    </div>
  </div>
  <div class="row">
    <div class="span9">
      <table id="accidents" class="table table-hover table-striped table-bordered" cellpadding="0" cellspacing="0"
             border="0" data-source="<%= admins_accidents_url(format: "json") %>">
        <thead>
        <tr class="alert alert-success">
          <th>驾驶员</th>
          <th>驾驶车辆</th>
          <th>肇事地点</th>
          <th>肇事时间</th>
          <th>事故责任人</th>
          <th>损坏程度</th>
          <th>伤亡人数</th>
          <th>更改</th>
        </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
  </div>
</div>

